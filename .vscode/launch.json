{
    "version": "0.2.0",
    "configurations": [{
            "name": "RunnerCLI UpdateRegistry",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Client/CLI/AV.Client.CLI.Runner/bin/Debug/netcoreapp2.0/RunnerCLI.dll",
            "args": [
                "/Process=UpdateRegistry",
                "/SubKeys=SOFTWARE\\ProgLan\\Product,SOFTWARE\\Wow6432Node\\ProgLan\\Product",
                "/Key=LogToDirectory",
                "/Value=C:\\Avi\\Logs"
            ],
            "cwd": "${workspaceFolder}/Client/CLI/AV.Client.CLI.Runner",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "RunnerCLI ToJson",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Client/CLI/AV.Client.CLI.Runner/bin/Debug/netcoreapp2.0/RunnerCLI.dll",
            "args": [
                "/Process=ToJson",
                "/FilePath=C:/AVI/Practice/ProgLan/Main/VDrive/Client/Console/RunnerCore/Client/CLI/AV.Client.CLI.Runner/bin/Debug/netcoreapp2.0/Windows/UpdateRoles/India.xml"
            ],
            "cwd": "${workspaceFolder}/Client/CLI/AV.Client.CLI.Runner",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "RunnerWebAPI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Service/WebAPI/AV.Service.WebAPI.Runner/bin/Debug/netcoreapp2.0/AV.Service.WebAPI.Runner.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}